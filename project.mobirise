    "privacidade.html": {
      "settings": {
        "meta_descr": "A Plan4Sustain é especializada em sustentabilidade e na definição de novos planos de negócio. Conheça a nossa Política de Privacidade.",
        "title": "Política de Privacidade - Plan4Sustain",
        "order": 5
      },
      "components": [
        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "background-color": "@bgColor",
